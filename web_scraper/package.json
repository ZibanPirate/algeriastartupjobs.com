{
  "name": "algeriastartupjobs",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "transpile": "tsc",
    "transpile:watch": "tsc -w",
    "ele": "electron ./dist/_entry",
    "ele:watch": "nodemon --watch ./dist --exec \"electron ./dist/_entry\"",
    "dev": "npm-run-all transpile --parallel transpile:watch ele:watch"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "electron": "^27.0.2",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
